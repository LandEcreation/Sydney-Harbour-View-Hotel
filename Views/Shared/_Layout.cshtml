<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title> Sydney Harbor View Hotel</title>
    <script src="https://use.fontawesome.com/98fb124257.js"></script>

    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <!--google font -->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@900&display=swap" rel="stylesheet">



    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <section id="nav-bar">
        <div class="navbar navbar-inverse navbar-expand-lg navbar-fixed-top">
            <div class="container-fluid">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a href="/home" class="navbar-brand">    <img src="~/Images/mylogo.png" />          </a>

                    @*@Html.ActionLink("Sydney HArbour View", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })*@
                </div>
                <div class="navbar-collapse collapse">
                    <ul class="nav navbar-nav me-auto">

                        <li class="nav-item">
                            <a class="nav-link active" href="/Home">Home</a>
                        </li>

                        <li class="nav-item">
                            <a class="nav-link active" href="/Home/About">About US</a>
                        </li>


                        <li class="nav-item">
                            <a class="nav-link active" href="/Parties/Events">Events</a>
                        </li>


                        <li class="nav-item">
                            <a class="nav-link active" href="/Attendees/Create">Booking</a>
                        </li>






                        <li class="nav-item">
                            <a class="nav-link active" href="/Home/Contact">Contact US</a>
                        </li>


                    </ul>
                </div>
            </div>
        </div>


    </section>





    <div class="body-content">
        @RenderBody()

    </div>




    <section id="footer">
        <div class="container-fluid">
            <div class="row">

                <div class="col-lg-4 footer-p">
                    <h4> Phone Support </h4>
                    <p> 24 hours </p>
                    <p1> +61 405 326 790 </p1>
                </div>

                <div class="col-lg-4 footer-p">
                    <h4> Follow Us </h4>
                    <p> Social medial channel </p>
                    <div class="social-icon">
                        <a href="https://www.facebook.com/yonungbmud" target="_blank"> <i class="fa fa-facebook-square fa-3x  social-media"></i> </a>
                        <a href="https://www.instagram.com/" target="_blank"> <i class="fa fa-instagram fa-3x social-media"></i> </a>
                        <a href="https://www.linkedin.com/feed/" target="_blank"> <i class="fa fa-twitter fa-3x social-media"></i> </a>
                    </div>
                </div>


                <div class="col-lg-4 footer-p">
                    <h4>Our Newsletter</h4>
                    <p> Sign up for details </p>
                    <input type="text" name="" value="" placeholder="Email..."> <input type="submit" class="btn btn-warning subscribe-btn" name="button" value="Subscribe">
                </div>
            </div>

            <p class="copyright">
                &copy; CopyRight Reserved to Sydney Harbour View hotel.com.au &reg;<br>
                <span class="designer"> Designed With ❤️ by <a href="https://www.linkedin.com/in/liladhar-sapkota-6342b518a/"> Liladhar </a> </span>
            </p>


        </div>
    </section>


















    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
